{
  "name": "taxpayer-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dashboard": "ng build --project=dashboard --prod --output-hashing none --single-bundle true",
    "cdn:dashboard": "http-server ./dist/dashboard -p 8081",
    "npx-build-plus:copy-assets": "node projects/shell/copy-bundles.js",
    "build:dashboard:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project dashboard --single-bundle --output-hashing none",
    "build:shell:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project shell --single-bundle --output-hashing none",
    "serve:shell:externals":"ng s --project shell --prod --extra-webpack-config webpack.externals.js --proxy-config proxy.conf.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/elements": "^7.2.15",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@webcomponents/custom-elements": "^1.2.4",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "classlist.js": "^1.1.20150312",
    "copy": "^0.3.2",
    "core-js": "^2.5.4",
    "document-register-element": "1.8.1",
    "ngx-build-plus": "^7.8.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "http-serve": "^1.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
